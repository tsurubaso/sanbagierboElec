{
  "name": "sanbagierboelec",
  "private": true,
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "electron:dev": "npx electron . --dev",
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 --timeout 600000 && electron . --dev\"",
    "dev2": "concurrently \"npm run vite\" \"npm run electron:dev\"",
    "dev3": "concurrently -k \"npm run vite\" \"npm run electron:dev\"",
    "start": "electron .",
    "build": "vite build && electron-builder"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "dotenv": "^17.2.3",
    "electron-store": "^11.0.2",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "rehype-raw": "^7.0.0",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "vite": "^7.1.7",
    "wait-on": "^9.0.3"
  }
}
